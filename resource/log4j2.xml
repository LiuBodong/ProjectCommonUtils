<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info" strict="true">
    <Appenders>
        <Appender type="File" name="File" fileName="out.log">
            <Layout type="PatternLayout">
                <Pattern>%d %p %C{1.} [%t] %m%n</Pattern>
            </Layout>
        </Appender>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="File"/>
        </Root>
    </Loggers>
</Configuration>